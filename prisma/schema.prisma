// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Country {
  id                Int @id @default(autoincrement())
  Country_name      String
  water_quality     Int
  Water_comsumption Int
}

model Continent {
  id                Int @id @default(autoincrement())
  Continent_name    String
  water_quality     Int
  Water_comsumption Int
}

model User {
  id       Int     @id @default(autoincrement())
  email    String  @unique
  name     String?
  password String?
  votes    Vote[]
}

model Vote {
  id         Int  @id @default(autoincrement())
  user       User @relation(fields: [userId], references: [id])
  userId     Int
  water_vote Int
}
